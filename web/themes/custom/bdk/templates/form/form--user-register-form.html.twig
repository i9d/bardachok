{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}
<div class="auth-form" id="user-register-form-wrapper">
  <div class="form-header">
    <h2 class="auth-title">Зарегистрироваться</h2>
    <span class="has-account register-step-one">Уже есть аккаунт? <a href="/user">Войти</a></span>
  </div>
  <form{{ attributes }}>
    {{ children }}
  </form>
</div>
